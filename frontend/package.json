    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^23.0.1",
    "less": "^4.3.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.0",
    "typescript": "~5.3.3",
    "vite": "^7.1.7",
